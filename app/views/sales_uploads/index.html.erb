<p id="notice"><%= notice %></p>

<h1>Sales Uploads</h1>

<table>
  <thead>
    <tr>
      <th>Video</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @sales_uploads.each do |sales_upload| %>
      <tr>
        <td><%= sales_upload.video %></td>
        <td><%= link_to 'Show', sales_upload %></td>
        <td><%= link_to 'Edit', edit_sales_upload_path(sales_upload) %></td>
        <td><%= link_to 'Destroy', sales_upload, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Sales Upload', new_sales_upload_path %>
